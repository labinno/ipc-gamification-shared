{{#items}}
.{{name}} {
  background-image: url({{{escaped_image}}});
  background-position: {{px.offset_x}} {{px.offset_y}};
  width: {{px.width}};
  height: {{px.height}};
}
{{#custom}}
.customize-option.{{name}} {
  background-image: url({{{escaped_image}}});
  background-position: {{custom.px.offset_x}} {{custom.px.offset_y}};
  width: {{custom.px.width}};
  height: {{custom.px.height}};
}
{{/custom}}
{{/items}}